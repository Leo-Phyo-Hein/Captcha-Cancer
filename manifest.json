{
    "manifest_version": 2,
    "name": "Popup Extension",
    "description": "Displays a popup every few seconds.",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "browser_action":{
        "default_icon":"patrick.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["popup.js"]
        }
    ]
}